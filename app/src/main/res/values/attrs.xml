<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="PasswordEditText">
        <attr name="btn_visible" format="boolean" />
        <attr name="edit_hint" format="string" />
        <attr name="drawableStart" format="reference" />
    </declare-styleable>

    <declare-styleable name="BaselineTextView">
        <attr name="baseline_color" format="color" />
        <attr name="baseline_height" format="dimension" />
    </declare-styleable>

    <declare-styleable name="CodeView">
        <attr name="setText" format="string" />
    </declare-styleable>

    <declare-styleable name="MyProgressView">
        <attr name="progress" format="float"/>
    </declare-styleable>

    <declare-styleable name="RoundRectImageView">
        <attr name="radius" format="float" />
    </declare-styleable>

    <declare-styleable name="MyCalendarView">
        <attr name="mTextSizeMonth" format="dimension" />        <!--标题字体大小-->
        <attr name="mTextSizeWeek" format="dimension" />         <!--星期字体大小-->
        <attr name="mTextSizeDay" format="dimension" />          <!--日期字体大小-->

        <attr name="mTextColorMonth" format="color" />           <!--标题字体颜色-->
        <attr name="mTextColorDay" format="color" />             <!--日期字体颜色-->
        <attr name="mTextColorWeek" format="color" />            <!--星期字体颜色-->
        <attr name="mTextColorSelected" format="color" />        <!--当天日期颜色-->
    </declare-styleable>
</resources>