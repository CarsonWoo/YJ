<?xml version="1.0" encoding="utf-8"?>
<layer-list
    xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background" android:height="8dp">
        <shape android:shape="rectangle">
            <solid android:color="#63c8c8c8" />
            <corners android:radius="20dp" />
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress" android:height="10dp">
        <clip>
            <shape android:shape="rectangle">
                <corners android:radius="20dp" />
                <solid android:color="#63c8c8c8" />
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress" android:height="10dp">
        <clip>
            <shape android:shape="rectangle">
                <corners android:radius="20dp" />
                <solid android:color="#8d8d8d" />
            </shape>
        </clip>
    </item>
</layer-list>